{
    "name": "ripe-components-react-native-storybook",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "android-release": "react-native run-android --variant=release",
        "cleanup": "rm -rf node_modules && rm -f package-lock.json",
        "clear": "react-native start --reset-cache",
        "ios": "react-native run-ios",
        "ios-release": "react-native run-ios --configuration Release",
        "link": "react-native link",
        "lint": "eslint .",
        "lint-fix": "eslint \"./**/*.{js,jsx}\" --fix",
        "pod": "cd ios && pod update",
        "prettier": "prettier \"./**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "start": "react-native start",
        "prestorybook": "rnstl",
        "storybook": "start-storybook -p 7007",
        "test": "jest",
        "upgrade": "npx sort-package-json && ncu -u",
        "wml": "wml start",
        "wml-base": "wml",
        "wml-link": "wml add",
        "wml-link-components": "wml add ${RIPE_COMPONENTS_REACT_NATIVE_PATH-../ripe-components-react-native} node_modules/ripe-components-react-native"
    },
    "config": {
        "react-native-storybook-loader": {
            "searchDir": [
                "./node_modules/ripe-components-react-native/react/"
            ],
            "pattern": "**/*.stories.js",
            "outputFile": "./temp/storyLoader.js"
        }
    },
    "jest": {
        "preset": "react-native"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/clipboard": "^1.5.1",
        "@react-native-community/datetimepicker": "^3.5.0",
        "@react-native-community/masked-view": "^0.1.11",
        "@react-native-community/netinfo": "^6.0.0",
        "@react-native-picker/picker": "^1.16.1",
        "@sentry/react-native": "^2.6.1",
        "fast-text-encoding": "^1.0.3",
        "pusher-js": "^7.0.3",
        "react": "^17.0.2",
        "react-native": "^0.64.1",
        "react-native-device-info": "^8.1.3",
        "react-native-document-picker": "^5.0.4",
        "react-native-fs": "^2.18.0",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-image-picker": "^3.2.1",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-picker-select": "^8.0.4",
        "react-native-onesignal": "^4.2.0",
        "react-native-safe-area-context": "^3.2.0",
        "react-native-safe-area-view": "^1.1.1",
        "react-native-svg": "^12.1.1",
        "ripe-commons": "^0.4.0",
        "ripe-commons-native": "^0.2.0",
        "ripe-components-react-native": "^0.4.0",
        "ripe-sdk": "^1.25.9",
        "yonius": "^0.7.7"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/runtime": "^7.14.0",
        "@react-native-community/eslint-config": "^2.0.0",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-knobs": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/addon-ondevice-knobs": "^5.3.25",
        "@storybook/addons": "^6.2.9",
        "@storybook/react-native": "^5.3.25",
        "@storybook/react-native-server": "^5.3.23",
        "babel-jest": "^27.0.2",
        "babel-loader": "^8.2.2",
        "babel-plugin-inline-import": "^3.0.0",
        "eslint": "^7.27.0",
        "jest": "^27.0.3",
        "metro-react-native-babel-preset": "^0.66.0",
        "npm-check-updates": "^11.6.0",
        "prettier": "^2.3.0",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-storybook-loader": "^2.0.4",
        "react-test-renderer": "^17.0.2",
        "sort-package-json": "^1.50.0",
        "typescript": "^4.3.2",
        "wml": "^0.0.83"
    }
}
