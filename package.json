{
    "name": "ripe-components-react-native-storybook",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "android-release": "react-native run-android --variant=release",
        "cleanup": "rm -rf node_modules && rm -f package-lock.json",
        "clear": "react-native start --reset-cache",
        "ios": "react-native run-ios",
        "ios-release": "react-native run-ios --configuration Release",
        "link": "react-native link",
        "lint": "eslint .",
        "lint-fix": "eslint \"./**/*.{js,jsx}\" --fix",
        "pod": "cd ios && pod update",
        "prettier": "prettier \"./**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "start": "react-native start",
        "prestorybook": "rnstl",
        "storybook": "start-storybook -p 7007",
        "test": "jest",
        "upgrade": "npx sort-package-json && ncu -u",
        "wml": "wml start",
        "wml-base": "wml",
        "wml-link": "wml add",
        "wml-link-components": "wml add ${RIPE_COMPONENTS_REACT_NATIVE_PATH-../ripe-components-react-native} node_modules/ripe-components-react-native"
    },
    "config": {
        "react-native-storybook-loader": {
            "searchDir": [
                "./node_modules/ripe-components-react-native/react/"
            ],
            "pattern": "**/*.stories.js",
            "outputFile": "./temp/storyLoader.js"
        }
    },
    "jest": {
        "preset": "react-native"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.11.0",
        "@react-native-community/masked-view": "^0.1.10",
        "react": "^16.13.1",
        "react-native": "^0.61.5",
        "react-native-device-info": "^5.6.3",
        "react-native-document-picker": "^3.5.4",
        "react-native-image-picker": "^2.3.3",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-safe-area-context": "^3.1.1",
        "react-native-safe-area-view": "^1.1.1",
        "react-native-svg": "^12.1.0",
        "ripe-components-react-native": "^0.2.16"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/runtime": "^7.10.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-knobs": "^5.3.19",
        "@storybook/addon-links": "^5.3.19",
        "@storybook/addon-ondevice-knobs": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/react-native": "^5.3.19",
        "@storybook/react-native-server": "^5.3.19",
        "babel-jest": "^26.2.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-inline-import": "^3.0.0",
        "eslint": "^7.5.0",
        "jest": "^26.2.1",
        "metro-react-native-babel-preset": "^0.61.0",
        "npm-check-updates": "^7.0.2",
        "prettier": "^2.0.5",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-storybook-loader": "^2.0.2",
        "react-test-renderer": "^16.13.1",
        "sort-package-json": "^1.44.0",
        "typescript": "^3.9.7",
        "wml": "^0.0.83"
    }
}
