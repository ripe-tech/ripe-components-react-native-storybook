{
    "name": "ripe-components-react-native-storybook",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "cleanup": "rm -rf node_modules && rm -f package-lock.json",
        "start": "react-native start",
        "android": "react-native run-android",
        "android-release": "react-native run-android --variant=release",
        "ios": "react-native run-ios",
        "link": "react-native link",
        "test": "jest",
        "lint": "eslint .",
        "lint-fix": "eslint \"./**/*.{js,jsx}\" --fix",
        "pod": "cd ios && pod update",
        "prettier": "prettier \"./**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "wml": "wml start",
        "wml-link": "wml add",
        "wml-link-components": "wml add ${RIPE_COMPONENTS_REACT_NATIVE_PATH-../ripe-components-react-native} node_modules/ripe-components-react-native",
        "wml-base": "wml",
        "storybook": "start-storybook -p 7007",
        "prestorybook": "rnstl"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.7.1",
        "@react-native-community/masked-view": "^0.1.6",
        "react": "^16.12.0",
        "react-native": "^0.61.5",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-safe-area-context": "^0.7.2",
        "react-native-safe-area-view": "^1.0.0",
        "react-native-svg": "^11.0.1",
        "ripe-components-react-native": "^0.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/runtime": "^7.8.4",
        "@react-native-community/eslint-config": "^0.0.7",
        "@storybook/addon-actions": "^5.3.13",
        "@storybook/addon-knobs": "^5.3.13",
        "@storybook/addon-links": "^5.3.13",
        "@storybook/addon-ondevice-knobs": "^5.3.13",
        "@storybook/addons": "^5.3.13",
        "@storybook/react-native": "^5.3.13",
        "@storybook/react-native-server": "^5.3.13",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-inline-import": "^3.0.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.58.0",
        "prettier": "^1.19.1",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-storybook-loader": "^1.8.1",
        "react-test-renderer": "^16.12.0",
        "wml": "^0.0.83"
    },
    "jest": {
        "preset": "react-native"
    },
    "config": {
        "react-native-storybook-loader": {
            "searchDir": [
                "./node_modules/ripe-components-react-native/react/"
            ],
            "pattern": "**/*.stories.js",
            "outputFile": "./temp/storyLoader.js"
        }
    }
}
